{
  "name": "onnxjs",
  "version": "0.1.0",
  "description": "A Javascript library for running ONNX models on browsers and on Node.js",
  "main": "./lib/api/index.js",
  "browser": "./dist/onnx.min.js",
  "types": "./types/lib/api/index.d.ts",
  "scripts": {
    "prepare": "tsc && node tools/build",
    "prepack": "node tools/build --build-wasm --build-bundle",
    "build": "tsc && node tools/build --build-wasm --build-bundle",
    "build:node": "tsc",
    "build:wasm": "node tools/build --build-wasm",
    "build:bundle": "node tools/build --build-bundle",
    "test": "node tools/test-runner-cli",
    "lint": "tslint -p . -t verbose",
    "format": "clang-format --glob=\"{{lib,test,tools}/**/*.ts,src/**/*.{cpp,h}}\" --style=file -i"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/onnxjs.git"
  },
  "keywords": [
    "ONNX.js"
  ],
  "author": "fs-eire",
  "license": "MIT",
  "dependencies": {
    "ndarray": "^1.0.18",
    "ndarray-gemm": "^1.0.0",
    "ndarray-ops": "^1.2.2",
    "onnx-proto": "^3.1.1",
    "platform": "^1.3.5"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/globby": "^8.0.0",
    "@types/karma": "^3.0.0",
    "@types/long": "^4.0.0",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.5",
    "@types/ndarray": "^1.0.6",
    "@types/npmlog": "^4.1.1",
    "@types/platform": "^1.3.1",
    "@types/rimraf": "^2.0.2",
    "@types/strip-json-comments": "0.0.30",
    "@types/webgl2": "0.0.4",
    "chai": "^4.2.0",
    "clang-format": "^1.2.4",
    "electron": "^3.0.9",
    "globby": "^8.0.1",
    "hard-source-webpack-plugin": "^0.12.0",
    "husky": "^1.1.4",
    "karma": "^3.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-edge-launcher": "^0.4.2",
    "karma-electron": "^6.0.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "long": "^4.0.0",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "npmlog": "^4.1.2",
    "numpy-parser": "^1.0.1",
    "rimraf": "^2.6.2",
    "strip-json-comments": "^2.0.1",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typescript": "^3.1.6",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "worker-loader": "^2.0.0"
  }
}
